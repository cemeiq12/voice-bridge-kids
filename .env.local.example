# ElevenLabs API Configuration
# Get your API key from: https://elevenlabs.io/
ELEVENLABS_API_KEY=your_api_key_here
NEXT_PUBLIC_ELEVENLABS_API_KEY=your_api_key_here

# Optional: Voice ID (default voice if not specified)
# Rachel (clear, neutral) - Great for therapy
ELEVENLABS_DEFAULT_VOICE_ID=21m00Tcm4TlvDq8ikWAM

# Optional: Model ID
# Options: eleven_monolingual_v1, eleven_multilingual_v2, eleven_turbo_v2
ELEVENLABS_MODEL_ID=eleven_monolingual_v1

# OpenAI Whisper API (for Speech-to-Text)
OPENAI_API_KEY=your_openai_api_key_here

# GPT-4o API (for speech correction in Bridge Mode)
OPENAI_GPT4_API_KEY=your_openai_gpt4_api_key_here
